extends ../layouts/main.pug

block content
    .top
        form(action="/create", method="get")
            button(type="submit") 추가하기

    each data in list
        .main
            .main__desc #{data.description}
            .main__footer 
                form(action=`/detail`, method="get")
                    input(type="hidden" name="id" value=data._id)
                    button(type="submit") 상세보기